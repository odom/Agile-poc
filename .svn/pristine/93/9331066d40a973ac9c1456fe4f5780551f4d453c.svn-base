<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<meta type="array">
		<noEditOnCreation>1</noEditOnCreation>
		<langDisable>1</langDisable>
		<default type="array">
			<TCEForms type="array">
				<sys_language_uid>-1</sys_language_uid>
			</TCEForms>
		</default>
	</meta>
	<ROOT type="array">
		<tx_templavoila type="array">
			<title>ROOT</title>
			<description>Select the HTML element on the page which you want to be the overall container element for the template.</description>
		</tx_templavoila>
		<type>array</type>
		<el type="array">
			<field_col1 type="array">
				<tx_templavoila type="array">
					<title>Left Content</title>
					<sample_data type="array">
						<numIndex index="0"></numIndex>
					</sample_data>
					<eType>ce</eType>
					<TypoScript>
						10= RECORDS
						10.source.current=1
						10.tables = tt_content
					</TypoScript>
					<preview></preview>
					<enableDragDrop type="integer">1</enableDragDrop>
				</tx_templavoila>
				<TCEforms type="array">
					<label>Left Content</label>
					<config type="array">
						<type>group</type>
						<internal_type>db</internal_type>
						<allowed>tt_content</allowed>
						<size>5</size>
						<maxitems>200</maxitems>
						<minitems>0</minitems>
						<multiple>1</multiple>
						<show_thumbs>1</show_thumbs>
					</config>
				</TCEforms>
			</field_col1>
			<field_col2 type="array">
				<tx_templavoila type="array">
					<title>Right Content</title>
					<sample_data type="array">
						<numIndex index="0"></numIndex>
					</sample_data>
					<eType>ce</eType>
					<TypoScript>
						10= RECORDS
						10.source.current=1
						10.tables = tt_content
					</TypoScript>
					<preview></preview>
					<enableDragDrop type="integer">1</enableDragDrop>
				</tx_templavoila>
				<TCEforms type="array">
					<label>Right Content</label>
					<config type="array">
						<type>group</type>
						<internal_type>db</internal_type>
						<allowed>tt_content</allowed>
						<size>5</size>
						<maxitems>200</maxitems>
						<minitems>0</minitems>
						<multiple>1</multiple>
						<show_thumbs>1</show_thumbs>
					</config>
				</TCEforms>
			</field_col2>
		</el>
	</ROOT>
</T3DataStructure>